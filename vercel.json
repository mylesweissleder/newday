{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://network-crm-api.onrender.com/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "client/dist/$1"
      }
    ]
  }
