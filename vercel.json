{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://network-crm-backend.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/dist",
  "env": {
    "VITE_API_URL": "https://api.whatintheworldwasthat.com"
  },
  "domains": ["whatintheworldwasthat.com", "www.whatintheworldwasthat.com"]
}